generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Place {
  id          Int     @id @default(autoincrement())
  fk_place    Int
  node        Int
  name        String  @unique
  
  email       String
  website     String
  opening_hours String

  lat         Float
  long        Float
  addr_city   String
  addr_housenumber  Int
  addr_postcode   Int
  addr_street String
  
  cuisine     String
  diet_vegan Boolean @default(false)
  diet_vegetarian Boolean @default(false)

  created DateTime  @default(now())
  updated DateTime  @default(now())
}